{
  "name": "telemetry-web",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://Tam@github.com/ethercreative/telemetry-web.git",
  "author": "Tam <hi@tam.sx>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "nodemon -L -e js,twig,json server.js",
    "serve": "pm2 start server.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nunjucks": "^3.2.0",
    "pm2": "^4.2.1",
    "postgres": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  },
  "build": {
    "copy": {
      "run": true,
      "paths": {
        "assets/fonts": "assets/fonts",
        "assets/svg": "assets/svg"
      }
    },
    "critical": {
      "run": false,
      "baseUrl": "https://dev.telemetry",
      "cssUrl": "/assets/css/[style.scss]",
      "paths": {
        "index": "/",
        "_view": "/simplemap",
        "404": "/404"
      }
    }
  }
}
